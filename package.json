{
  "name": "groovelauncher",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack --watch & sass --no-source-map --watch src/styles.scss dist/styles.css",
    "build": "npx webpack & sass --no-source-map src/styles.scss dist/styles.css",
    "flash": "npx webpack && sass --no-source-map src/styles.scss dist/styles.css && sh flash.sh && echo Finished",
    "multiflash": "npx webpack && sass --no-source-map src/styles.scss dist/styles.css && sh flash.sh --multi && echo Finished"
  },
